<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vms="clr-namespace:WeddingManager.ViewModels"
                    xmlns:views="clr-namespace:WeddingManager.Views"
                    xmlns:vmsmt="clr-namespace:WeddingManager.ViewModels.MainTabs"
                    xmlns:viewsmt="clr-namespace:WeddingManager.Views.MainTabs"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    
    <!--Main Layout-->
    <DataTemplate DataType="{x:Type vms:MainLayoutViewModel}">
        <views:MainLayout />
    </DataTemplate>

    <!--Project Selector-->
    <DataTemplate DataType="{x:Type vms:ProjectSelectorViewModel}">
        <views:ProjectSelectorView />
    </DataTemplate>

    <!--Main Tabs-->
    <DataTemplate DataType="{x:Type vmsmt:VisitorsViewModel}">
        <viewsmt:VisitorsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vmsmt:ToastsViewModel}">
        <viewsmt:ToastsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vmsmt:CounterpartiesViewModel}">
        <viewsmt:CounterpartiesView />
    </DataTemplate>

    
    <sys:Guid x:Key="EmptyGuid">00000000-0000-0000-0000-000000000000</sys:Guid>

</ResourceDictionary>