<Window x:Class="WeddingManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vms="clr-namespace:WeddingManager.ViewModels"
        xmlns:views="clr-namespace:WeddingManager.Views"
        Title="Wedding Manager" Height="350" Width="525">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Dictionaries\ResourceDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>

    <ContentPresenter>
        <ContentPresenter.Content>
            <vms:MainLayoutViewModel />
        </ContentPresenter.Content>
    </ContentPresenter>
</Window>
